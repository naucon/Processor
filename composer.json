{
    "name": "naucon/processor",
    "description": "This package provides a generic processor chain to process a given object with processors/task (kind of a pipline, command chain pattern).",
    "keywords": ["command", "processor", "chain", "tasks", "pipeline"],
    "homepage": "https://github.com/naucon/Processor",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sven Sanzenbacher"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "naucon/utility": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "psr-4": {"Naucon\\Processor\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Naucon\\Processor\\Tests\\": "tests"}
    }
}
